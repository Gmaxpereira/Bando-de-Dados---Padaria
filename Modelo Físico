CREATE TABLE duplicata (

cod_barras VARCHAR(20) (PK)
data_gera DATE
data_venc DATE
situacao VARCHAR(20)
valor FLOAT
cnpj_fornecedor VARCHAR(14) (FK) 
)


CREATE TABLE fornecedor (

cnpj VARCHAR(14) (PK) 
raz_social VARCHAR(60)
nome_fantasia VARCHAR(60)
vendedor VARCHAR(60)
telefone VARCHAR(11)
forma_paga VARCHAR(20)
)

CREATE TABLE Materia_prima(

cod_mat_prima INTEGER (PK)
nome VARCHAR(40)
custo FLOAT
quantidade FLOAT 
data_entrega DAT
cnpj_fornecedor VARCHAR (14) (FK)
)


CREATE TABLE Almoxarifado (

cod_almox INTEGER (PK) 
nome VARCHAR(40)
custo FLOAT
quantidade FLOAT
cnjp_fornecedor VARCHAR(20) (FK)
)


CREATE TABLE Fornecedor_produto(

cnpj_fornecedor VARCHAR (14) (PK) (FK)
cod_produto INTEGER (PK) (FK)
)


CREATE TABLE materia_prima_receita(

cod_mat_prima INTEGER (PK) (FK)
cod_receita INTEGER (PK) (FK)
quantidade FLOAT
)


CREATE TABLE produto(

cod_produto INTEGER (PK)
cod_barras VARCHAR (20)
nome VARCHAR (50)
categoria VARCHAR (50)
quantidade FLOAT
custo FLOAT
preco FLOAT
quant_vendido FLOAT
cod_receita INTEGER NULL (FK)
)


CREATE TABLE receita(

cod_receita INTEGER (PK)
custo_bruto FLOAT
custo_op FLOAT
categoria VARCHAR (30)
)


CREATE TABLE endereco(

cep INTERGER
rua VARCHAR (70)
numero VARCHAR (6)
bairro VARCHAR (50)
cidade VARCHAR (70)
complemento VARCHAR (70)
cpf_cliente VARCHAR (11) (PK) (FK)
)


CREATE TABLE cliente(

cpf_cliente VARCHAR (11) (PK)
nome VARCHAR (90)
saldo FLOAT
telefone INTEGER 
possui_credito BOOLEAN
)


CREATE TABLE produto_venda(

cod_produto INTEGER (PK) (FK)
cod_venda INTEGER (PK) (FK)
quantidade FLOAT
)


CREATE TABLE cliente_venda(

cpf_cliente VARCHAR(11) (PK) (FK)
cod_venda INTEGER (PK) (FK)
)


CREATE TABLE venda(
cod_venda INTEGER (PK)
data_hora DATETIME
valor_total FLOAT 
)


CREATE TABLE fechamento_caixa (

data_fechamento DATE (PK)
valor_total FLOAT 
valor_cedulas INTEGER
valor_moedas FLOAT
valor_debito FLOAT
valor_credito FLOAT
codigo_venda INTEGER (FK)
)
